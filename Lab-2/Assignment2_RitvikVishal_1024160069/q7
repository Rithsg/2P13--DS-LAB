#include <iostream>
using namespace std;

int main() {
    int a[12] = {0, 1, 2, 3, 4, 5, 3, 4, 5, 6, 7, 8};
    int n = 12;
    int inversion = 0;

    for (int i = 0; i < n - 1; i++) {
        if (a[i] > a[i+1]) {
            inversion++;
		}
        
    }

    cout << "The number of inversions are " << inversion << endl;
    return 0;
}
