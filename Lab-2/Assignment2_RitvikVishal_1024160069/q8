#include <iostream>
using namespace std;



int main(){
	int n;
	cout << "No. of elements in array needed: ";
	cin >> n;
	int a[n],count;
	cout << "Enter the elements of the array: ";
	for(int i = 0;i < n;i++){
		cin >> a[i];
	}
	cout << "Your Array: ";
	for(int i = 0;i < n;i++){
		cout << a[i] << " ";
	}
	cout << "\n";
	
	for(int i = 0;i < n;i++){
		for(int j = i+1;j < n;j++){
			if(a[i] == a[j]){
				for(int k = j;k < n-1;k++){
					a[k] = a[k+1];
				}
				n--;
				i--;
			}
			
		}
	}
	cout << "Your no duplicated Array: ";
	for(int i = 0;i < n;i++){
		cout << a[i] << " ";
	}
	cout << "\n";
	cout << "No of unique elements in the array is " << n << endl;
	
	
}
