#include <iostream>
#include <vector>
using namespace std;

int main() {
    int n, m;
    cout << "Enter the number of elements (n): ";
    cin >> n;

    vector<int> a(n-1); 

    cout << "Enter the " << n-1 << " sorted elements (1 to n, one missing): ";
    for (int i = 0; i < n-1; i++) {
        cin >> a[i];
    }

   
    int missing = n;  
    for (int i = 0; i < n-1; i++) {
        if (a[i] != i+1) {
            missing = i+1;
            break;
        }
    }
    cout << "Missing number is: " << missing << endl;

  
    cout << "Enter the element you want to search: ";
    cin >> m;

    int low = 0, high = n-2;
    bool found = false;
    while (low <= high) {
        int mid = low + (high-low)/2;
        if (a[mid] == m) {
            cout << "The element " << m << " is at position " << mid+1 << endl;
            found = true;
            break;
        } else if (a[mid] < m) {
            low = mid+1;
        } else {
            high = mid-1;
        }
    }
    if (!found) cout << "Element not present" << endl;

    return 0;
}
