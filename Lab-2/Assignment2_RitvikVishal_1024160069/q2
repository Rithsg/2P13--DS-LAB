#include <iostream>
using namespace std;


int main(){
	int a[7] = {64,34,25,12,22,11,90};
	int n = (sizeof(a)/sizeof(a[0]))-1,temp;
	for(int i = 0;i<n;i++){
		for(int j = 0;j<n-i;j++){
			if(a[j] > a[j+1]){
				temp = a[j];
				a[j] = a[j+1];
				a[j+1] = temp;
			}
		}
	}
	cout << "Your Array:";
	for(int i = 0;i < 7;i++){
		cout << a[i] << " ";
	}
}
